<templateSet group="dong4j">
  <template name="todo" value="todo-dong4j : ($date$ $time$) [$SELECTION$]" description="todo 标记人 标记时间 原因" toReformat="true" toShortenFQNames="true">
    <variable name="date" expression="date(&quot;yyyy年MM月dd日&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
      <option name="HTML" value="true" />
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="fixme" value="fixme-dong4j : ($date$ $time$ [$SELECTION$])" description="fixme-dong4j 标记时间 原因" toReformat="false" toShortenFQNames="true">
    <variable name="date" expression="date(&quot;yyyy年MM月dd日&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
      <option name="HTML" value="true" />
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSON" value="true" />
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="jdoc" value="/**&#10; * &lt;p&gt;Company: 科大讯飞股份有限公司-四川分公司&lt;/p&gt;&#10; * &lt;p&gt;Description: $END$&lt;/p&gt;&#10; *&#10; * @author dong4j&#10; * @date $date$ $time$&#10; * @email sjdong3@iflytek.com&#10; */" description="快速生成类的 javadoc" toReformat="false" toShortenFQNames="true">
    <variable name="date" expression="date(&quot;yyyy-MM-dd&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
      <option name="HTML" value="true" />
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="li" value="log.info($END$)" description="log.info()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ld" value="log.debug($END$)" description="log.debug()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="lw" value="log.warn($END$)" description="log.warn()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="le" value="log.error($END$)" description="log.error()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>